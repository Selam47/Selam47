Dart Dili

//Şart Blokları
void main() {
  
  int puan = 55;
if(puan>=56)
{
  print("Geçti");
}
  else{
    print("Kaldı");
  }
  String not = 'A';

  switch(not){
    case "A": print("Geçer");
    case "B": print("NOTUNUZ");
  }
}
//*Listeleme
void main() {
  var ulkeler = new List(4);
  ulkeler[0] = "norvec";
  ulkeler[1] = "kanada";
  ulkeler[2] = "romanya";
  ulkeler[3] = "ingiltere";
  print(ulkeler);
}

//Döngüler
void main() {
  var arabalar = ["kia", "hyundai", "togg"];

  for (var i = 0; i < arabalar.length; i++) {
    print(arabalar[i]);
  }
}

//Fonksiyonlarr
void main() {
  selamVer();
  selamVer2("Mehmet");
}

void selamVer() {
  print("merhaba");
}

void selamVer2(String kullanici) {
  print("Selam" + kullanici);
}

//class Methodu
void main() {
  var personalManager = PersonalManager();
  personalManager.add();

  var customerManager = CustomerManager();
  var customer = new Customer("Selam", "Kaya");

  customerManager.add(customer);
}

class PersonalManager {
  void add() {
    print("eklendi...");
  }

  void guncelle() {
    print("güncellendi.");
  }
}

class CustomerManager {
  void add(Customer customer) {
    print("eklendi: " + customer.firstName);
  }

  void guncelle() {
    print("güncellendi.");
  }
}

class Customer {
  String firstName;
  String lastName;

  Customer(String firstName, String lastName) {
    this.firstName = firstName;
    this.lastName = lastName;
  }
}
